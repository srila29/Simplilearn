pipeline {
    agent any

    tools {
        // Install the Maven version configured as "M3" and add it to the path.
        maven "MAVEN_HOME"
    }

    stages {
        stage('Preparation') {
            steps {
                git branch: 'main', url: 'https://github.com/srila29/Simplilearn.git'
            }
        }
        stage('Build') {
            steps {
                //sh "mvn clean test"
                bat "mvn clean test"
            }
        }
    }

}
